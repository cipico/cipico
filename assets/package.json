{
  "name": "cipico",
  "version": "1.0.0",
  "description": "A subtheme of Octavia for use with the Bulma Drupal distribution",
  "main": "sass/mystyles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "author": "Nico Friedrich",
  "license": "GPL",
  "devDependencies": {
    "bulma": "^0.9.4",
    "bulma-extensions": "^3.0.0",
    "node-sass": "^4.9.4"
  },
  "dependencies": {
    "bulma-checkradio": "^1.1.1"
  }
}
